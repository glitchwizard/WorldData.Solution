@using WorldData.Models;
<h1>Countries</h1>



<ul>
    @foreach (Country country in Model)
    {
      <li><a href='/Countries/@country.CountryCode' id="@country.CountryCode">@country.CountryName</a></li>
    }
  }
</ul>



